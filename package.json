{
  "name": "shop",
  "version": "1.0.0",
  "description": "Shopping list desktop app",
  "main": "main.js",
  "scripts": {
      "start": "electron .",
      "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
      "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ShoppingList\"",
      "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"

  },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/lackhoa/shop.git"
    },
    "author": "Vo Dang Khoa",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lackhoa/shop/issues"
    },
    "homepage": "https://github.com/lackhoa/shop#readme",
    "dependencies": {
        "electron": "^4.0.1",
        "node.js": "0.0.0"
    },
    "devDependencies": {
        "electron-packager": "^13.0.1"
    }
}
